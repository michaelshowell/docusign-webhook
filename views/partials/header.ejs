<script type="text/javascript">
  $(document).ready(function(){
      $('input[type="radio"]').click(function(){
          if($(this).attr("value")=="customer"){
              $("#partnerInfos1").hide();
          } else {
              $("#partnerInfos1").show();
          }
      });
      $('input[type="checkbox"]').click(function(){
          if($(this).is(':checked')){
              $("#InputShowCase1,#LabelShowCase1").removeAttr('disabled');
          } else {
              $("#InputShowCase1,#LabelShowCase1").attr('disabled','disabled');   
          }
      });
  });
  </script>
  <script type="text/javascript">
    $(function() {
      var now = new Date();
      // Please set Kazmon time window
      var numdays = 1;
      var lastWeek = new Date(now.getTime()-numdays*24*60*60*1000);
      $('#datetimepicker1').datetimepicker({
        defaultDate: lastWeek
      });
      $('#datetimepicker2').datetimepicker({
        defaultDate: now
      });
    });
  </script>
  